<view class="data-v-24fe0bed"><view class="sunsin_picture_list data-v-24fe0bed"><block wx:for="{{upload_picture_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="sunsin_picture_item data-v-24fe0bed"><image hidden="{{!(item.upload_percent<100)}}" class="sunsin_width data-v-24fe0bed" src="{{item.path}}" mode="aspectFill"></image><image hidden="{{!(item.upload_percent==100)}}" class="sunsin_width data-v-24fe0bed" src="{{item.path_server}}" mode="aspectFill" data-idx="{{index}}" data-event-opts="{{[['tap',[['previewImgs',['$event']]]]]}}" bindtap="__e"></image><view hidden="{{!(item.upload_percent<100)}}" class="sunsin_upload_progress sunsin_width data-v-24fe0bed" data-index="{{index}}" data-event-opts="{{[['tap',[['previewImg',['$event']]]]]}}" bindtap="__e">{{item.upload_percent+"%"}}</view><text class="{{['del iconfont icon-shanchu data-v-24fe0bed',upImgConfig.delBtnLocation=='left'?'left':upImgConfig.delBtnLocation=='right'?'right':upImgConfig.delBtnLocation=='bleft'?'bleft':upImgConfig.delBtnLocation=='bright'?'bright':'right']}}" style="{{('color:'+upImgConfig.delIconText+';background-color:'+upImgConfig.delIconColor)}}" data-url="{{item.path_server}}" data-index="{{index}}" hidden="{{upImgConfig.isDelIcon||false}}" data-event-opts="{{[['tap',[['deleteImg',['$event']]]]]}}" bindtap="__e">×</text></view></block><view class="data-v-24fe0bed"><block wx:if="{{upImgConfig.iconReplace==''||upImgConfig.iconReplace==undefined}}"><view hidden="{{!(upload_picture_list.length<upImgConfig.count||upImgConfig.notli)}}" class="sunsin_picture_item sunsin_width data-v-24fe0bed"><view data-event-opts="{{[['tap',[['chooseImage',['$0'],['upImgConfig.count']]]]]}}" hidden="{{!(!upImgConfig.isAddImage||false)}}" class="sunsin_add_image sunsin_width data-v-24fe0bed" style="{{('background-color:'+upImgConfig.upBgColor+'')}}" bindtap="__e"><view class="data-v-24fe0bed"><text class="{{['iconfont data-v-24fe0bed',upImgConfig.upIconName==undefined?'icon-shangchuantupian':upImgConfig.upIconName]}}" style="{{'font-size:72rpx;margin-bottom:12rpx;'+('color:'+upImgConfig.upIconColor+';width:100%;')}}"></text></view><view class="icon-text data-v-24fe0bed" style="{{('color:'+upImgConfig.upIconColor+';width:100%;')}}">{{upImgConfig.upTextDesc==undefined||upImgConfig.upTextDesc==''?'上传照片':upImgConfig.upTextDesc}}</view></view></view></block><block wx:else><view hidden="{{!(upload_picture_list.length<upImgConfig.count||upImgConfig.notli)}}" class="sunsin_picture_item sunsin_width data-v-24fe0bed"><view data-event-opts="{{[['tap',[['chooseImage',['$0'],['upImgConfig.count']]]]]}}" hidden="{{!(upImgConfig.isAddImage||true)}}" class="sunsin_add_image sunsin_width data-v-24fe0bed" style="'background-color:#fff;" bindtap="__e"><image class="icon_replace data-v-24fe0bed" src="{{upImgConfig.iconReplace}}"></image></view></view></block></view></view></view>