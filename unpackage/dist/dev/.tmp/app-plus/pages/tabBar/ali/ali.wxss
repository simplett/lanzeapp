.lanzepadding {
  padding: 10rpx 30rpx;
}
.status {
  width: 100%;
  height: 0;
  position: fixed;
  z-index: 10;
  background-color: #fff;
  top: 0;
  height: var(--status-bar-height);
}
.header {
  width: 92%;
  padding: 0 4%;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  position: fixed;
  top: 0;
  z-index: 10;
  background-color: #fff;
  /* top: var(--status-bar-height); */
}
.header .title {
    font-size: 34rpx;
}
.place {
  background-color: #ffffff;
  height: 100rpx;
  margin-top: var(--status-bar-height);
}
.row {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
  /* align-items: flex-start; */
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
}
.row .title {
    font-size: 24rpx;
    width: 100%;
    margin-bottom: 20rpx;
}
.row-itemlist {
  text-align: center;
  width: 79.2px;
  margin: 10rpx 0rpx;
  font-size: 29.5rpx;
}
.row-item {
  width: 100%;
  background: #e2e3e5;
  height: 79.2px;
}
.myimage {
  width: 100%;
  height: 100%;
}

